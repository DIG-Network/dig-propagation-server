{
  "name": "dig-propagation-server",
  "version": "0.0.1-alpha.130",
  "description": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dig": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && npm run postbuild",
    "postbuild": "copyfiles -u 1 \"src/**/*.crt\" \"src/**/*.key\" dist",
    "start": "ts-node src/index.ts",
    "dev": "set DIG_USERNAME=1234 && set DIG_PASSWORD=1234 && ts-node src/index.ts",
    "server": "ts-node src/server.ts",
    "test": "mocha -r ts-node/register tests.ts",
    "prepare-release": "bash ./scripts/release.sh"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@dignetwork/datalayer-driver": "^0.1.28",
    "@dignetwork/dig-sdk": "^0.0.1-alpha.165",
    "async-mutex": "^0.5.0",
    "busboy": "^1.6.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "fs-extra": "^11.2.0",
    "node-cache": "^5.1.2",
    "request-ip": "^3.3.0",
    "tmp": "^0.2.3",
    "toad-scheduler": "^3.0.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/express": "^4.17.21",
